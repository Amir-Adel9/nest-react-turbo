{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", "build/**"]
    },
    "lint": { "dependsOn": ["^lint"] },
    "check-types": { "dependsOn": ["^check-types"] },
    "test": { "outputs": [] },
    "test:e2e": { "outputs": [] },
    "dev": { "cache": false, "persistent": true },
    "start": { "cache": false, "persistent": true },
    "preview": { "cache": false, "persistent": true },
    "install-browsers": {}
  }
}
